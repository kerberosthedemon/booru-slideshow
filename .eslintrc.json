{
  "env": {
    "browser": true,
    "es2020": true
  },
  "extends": "react-app",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {},
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}