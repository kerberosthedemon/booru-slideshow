{
  "name": "Booru-Slideshow",
  "version": "1.2",
  "description": "A booru based extension for image searching and browsing",
  "manifest_version": 2,
  "browser_action": {},
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "128": "icon192.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "*://*/*",
    "*://*/"
  ],
  "content_security_policy": "script-src 'self' 'sha256-5As4+3YpY62+l38PsxCEkjB1R4YtyktBtRScTJ3fyLU='; object-src 'self'"
}